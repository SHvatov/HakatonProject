<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: #191970;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.HelpController">
    <children>
        <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="-1.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@assets/Helpme.jpg" />
            </image>
        </ImageView>
        <Button fx:id="backButton" layoutX="28.0" layoutY="25.0" mnemonicParsing="false" onAction="#backMenue" prefHeight="72.0" prefWidth="127.0" style="-fx-background-color: #ffffff;" text="Back" textFill="MIDNIGHTBLUE">
            <font>
                <Font name="Verdana" size="31.0" />
            </font>
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
        </Button>
    </children>
</AnchorPane>
